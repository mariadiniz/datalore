<div class="row col s12">
  <div class="col s1"></div>
  <div class="col s6 card-panel">
    <%= line_chart Devolution.group_by_minute(:created_at).count, label: 'Chegada das devoluções', xtitle: "Horário de chegada", ytitle: "Devoluções"%>
  </div>
  <div class="col s4 card-panel">
    <%= pie_chart( Devolution.group(:status).count ) %>
  </div>
  <div class="col s1"></div>
</div>

<div class="row col s12">
  <div class="col s3"></div>
  <div class="col s6 card-panel"><%= bar_chart Devolution.group(:product).count %></div>
  <div class="col s3"></div>
</div>